{
  "release-type": "go",
  "include-v-in-tag": true,
  "draft": true,
  "versioning": "prerelease",
  "prerelease-type": "rc",
  "prerelease": true,
  "packages": {
    ".": {
      "extra-files": [
        {
          "type": "json",
          "path": "py/src/theorydb_py/version.json",
          "jsonpath": "$.version"
        },
        { "type": "json", "path": "ts/package.json", "jsonpath": "$.version" },
        {
          "type": "json",
          "path": "ts/package-lock.json",
          "jsonpath": "$.version"
        },
        {
          "type": "json",
          "path": "ts/package-lock.json",
          "jsonpath": "$.packages[''].version"
        }
      ]
    }
  }
}
